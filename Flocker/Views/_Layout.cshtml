

    <html xmlns="http://www.w3.org/1999/xhtml">


    <head>
        <title>@ViewBag.Title</title>
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no autoRotate:disabled">

        <script src="https://kit.fontawesome.com/3d37a0422a.js" crossorigin="anonymous"></script>
        <script src="https://code.iconify.design/1/1.0.6/iconify.min.js"></script>
        <script src="https://code.iconify.design/1/1.0.6/iconify.min.js"></script>

        <link href="https://fonts.googleapis.com/css2?family=Ubuntu:wght@400;500;700&display=swap" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@100;300;400;500;700;900&display=swap" rel="stylesheet">
        <script src="https://code.iconify.design/1/1.0.6/iconify.min.js"></script>
        <link href="~/content/reset.css" rel="stylesheet" />
        <script src="https://code.iconify.design/1/1.0.6/iconify.min.js"></script>

        <link href="~/content/ProductStyleSheet.css" rel="stylesheet" />
        <link href="~/content/homeStyleSheet.css" rel="stylesheet" />
        <link href="~/content/NavStyleSheet.css" rel="stylesheet" />
        <link href="~/content/StyleSheet.css" rel="stylesheet" />


    </head>





    <body>


     
        <nav>


            <div class="nav-container row justify-around align-center">


                <a asp-controller="Home" asp-action="Index" class="logo col-2">Flocker</a>

                <div class="searchbox-container col-10">
                    <i class="fa fa-search search-icon row justifiy-center align-center" aria-hidden="true"></i>

                    <input type="text" id="searchBox" placeholder="What are you looking for..." autocomplete="off" required/>
                    <ul class="searchResult">
                    </ul>
                    <span class="field-validation-error searcherror">No Results Found</span>
                </div>
         


                <partial name="_LoginPartial" />


            </div>



        </nav>


        <div class="main-container">
            @RenderBody()

        </div>
        <footer>
            <div class="footer-content container">
                <h1>DEMO WEBSITE CREATED BY Abdul Mudhir</h1>
            </div>
        </footer>



        @RenderSection("Scripts", required: false)

    </body>





    </html>

    <script src="~/script/searchbox.js"></script>